{
  "$schema": "../../node_modules/ng-packagr/ng-package.schema.json",
  "dest": "../../dist/nw-style-guide",
  "assets": [
    {
      "glob": "**/*",
      "input": "src/assets",
      "output": "assets"
    },
    {
      "glob": "**/*",
      "input": "src/lib/sass",
      "output": "sass"
    }
  ],
  "lib": {
    "entryFile": "src/public-api.ts"
  }
}