  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --prod",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "ghpages": "ng build --prod --output-path docs  --base-href /style-guide/",
    "package": "npm run clean && npm run compile && npm run copy",
    "clean": "rm -rf distribution",
    "compile": "ngc -p src/_lib/modules/picker/tsconfig.build.json",
    "copy": "npm run copy:sass & npm run copy:package_file",
    "copy:sass": "cp -R src/_lib/sass distribution",
    "copy:package_file": "cp -R package.json distribution",
    "postversion": "npm run copy:package_file"
  },
  "private": false,
  "devDependencies": {
    "@angular/animations": "^5.0.0",
    "@angular/cli": "^1.6.3",
    "@angular/common": "^5.0.0",
